<?xml version="1.0" ?>

<robot name="ra605">

  <!-- the "base_link" name is mandatory for urdf_tutorial -->
  <link name="base_link">
    <visual>
      <origin rpy="0 0 0" xyz="0 0 0.1"/>
      <geometry>
        <cylinder length="0.375" radius="0.105"/>
      </geometry>
    </visual>
  </link>
  <link name="link1" />
  <link name="link2" >
    <visual>
      <origin rpy="0 1.570796325 0" xyz="0.1 0 0"/>
      <geometry>
        <cylinder length="0.34" radius="0.090"/>
      </geometry>
    </visual>
  </link>
  <link name="link3" >
    <visual>
      <origin rpy="0 1.570796325 0" xyz="0.125 0 0.04"/>
      <geometry>
        <cylinder length="0.338" radius="0.08"/>
      </geometry>
    </visual>
  </link>
  <link name="link4" />
  <link name="link5" >
    <visual>
      <origin rpy="0 0 0" xyz="0.043 0 0"/>
      <geometry>
        <box size="0.086 0.16 0.070"/>
      </geometry>
    </visual>
  </link>
  <link name="link6" />

  <joint name="joint1" type="revolute">
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz="0 0 0.375" rpy="0 0 0" />
    <axis xyz="0 0 1"/>
    <limit effort="0" lower="-4.00" upper="1.3" velocity="0.555"/>
  </joint>

  <joint name="joint2" type="revolute">
    <parent link="link1"/>
    <child link="link2"/>
    <origin xyz="0.03 0 0" rpy="0 0 0" />
    <axis xyz="0 -1 0"/>
    <limit effort="0" lower="-0.138" upper="3.05" velocity="0.555"/>
  </joint>

  <joint name="joint3" type="revolute">
    <parent link="link2"/>
    <child link="link3"/>
    <origin xyz="0.34 0 0" rpy="0 0 0" />
    <axis xyz="0 -1 0"/>
    <limit effort="0" lower="-1.9188" upper="1.9188" velocity="0.555"/>
  </joint>

  <joint name="joint4" type="revolute">
    <parent link="link3"/>
    <child link="link4"/>
    <!-- <origin xyz="0.338 0 0.04" rpy="1.570796325 0 0" /> -->
    <origin xyz="0.338 0 0.04" rpy="0 0 0" />
    <axis xyz="1 0 0"/>
    <limit effort="0" lower="-3.1416" upper="3.1416" velocity="0.555"/>
  </joint>

  <joint name="joint5" type="revolute">
    <parent link="link4"/>
    <child link="link5"/>
    <origin xyz="0 0 0" rpy="0 0 0" />
    <axis xyz="0 -1 0"/>
    <limit effort="0" lower="-1.9188" upper="1.9188" velocity="0.555"/>
  </joint>
  
  <joint name="joint6" type="revolute">
    <parent link="link5"/>
    <child link="link6"/>
    <origin xyz="0.086 0 0" rpy="0 0 0" />
    <axis xyz="1 0 0"/>
    <limit effort="0" lower="-3.1416" upper="3.1416" velocity="0.555"/>
  </joint>
  
</robot>
